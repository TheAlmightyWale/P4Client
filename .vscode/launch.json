{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron-forge.cmd",
      "args": ["start", "--inspect-electron"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/.vite/build/**/*.js"],
      "outputCapture": "std"
    }
  ]
}
